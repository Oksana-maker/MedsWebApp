{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "js-cookie@3.0.1",
      "destination": "wwwroot/lib/js-cookie/",
      "files": [
        "dist/js.cookie.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/",
      "files": [
        "moment.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-datetimepicker@4.17.47",
      "destination": "wwwroot/lib/bootstrap-datetimepicker/",
      "files": [
        "js/bootstrap-datetimepicker.min.js",
        "css/bootstrap-datetimepicker-standalone.min.css",
        "css/bootstrap-datetimepicker.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "eonasdan-bootstrap-datetimepicker@4.17.49",
      "destination": "wwwroot/lib/eonasdan-bootstrap-datetimepicker/",
      "files": [
        "js/bootstrap-datetimepicker.min.js",
        "css/bootstrap-datetimepicker.min.css"
      ]
    }
  ]
}